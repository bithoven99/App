<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.see.asus.sa3ed3amer">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity android:name=".SplachScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".Vedioview"
            android:configChanges="orientation|screenSize"
            android:theme="@style/omartheme" />
        />
        <activity
            android:name=".Musicview"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Playlists"
            android:theme="@style/AppTheme" />
        <activity android:name=".Musicplaylist" />
        <activity android:name=".Help" />
        <activity android:name=".FileConversionTool"></activity>
    </application>

</manifest>